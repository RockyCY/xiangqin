}


.floatViewBG{
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background-color: transparent;
  z-index: 999;
}
.coverViewBG{
  position: absolute;
  width: 100%;
  height: 100%;
  background:#000000;
  opacity: 0.75;
}

.personalFillBG {
  position: absolute;
  height: 355px;
  left: 0px;
  right: 0px;
  bottom: 0px;
  opacity: 1;
  background-color: #fff;
  border: 1px solid transparent;
  border-radius: 22px 22px 0px 0px;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: center; */
}

.bottomBar{
    width: 100%;
