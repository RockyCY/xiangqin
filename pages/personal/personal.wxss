}

.otherMainTitleBG {
  height: 60px;
}

.otherMainTitleBG1 {
  height: 80px;
}

.personalMainTitle {
  margin-left: 18.5px;
  font-size: 20px;
  color: #9c9c9c;
  font-weight: bold;
}

.otherMainTitle {
  margin-top: 25px;
}

.otherMainTitle1 {
