.personalHeadBG {
  width: 100%;
  margin-top: 12px;
  height: 199px;
  background:transparent;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.personalImage {
   position: absolute;
   margin-top: 5px;
   width: 143px;
   height: 143px;
   border-radius: 71.5px;
   border-style: none;
   border-width: 0cm;
   background-color: #fff;
   overflow: hidden;
}

.personalImageMask {
  position: absolute;
  top: 0px;
  left: 0px;
  background-color: #000000;
  opacity: 0.6;
  width: 143px;
  height: 143px;
  border-radius: 50%;
}

.personalImage1 {
  position: absolute;
  top: 51.5px;
  left: 51.5px;
  width: 40px;
  height: 40px;
}

.personalImageText {
  font-size: 20px;
  margin-top: 160px;
}

.personalMainTitleBG {
  width: 100%;
  height: 45px;
  background:transparent;
